
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option ifname 'eth8 eth7 eth6 eth4'
	option proto 'static'
	option type 'bridge'
	option ipaddr '192.168.2.1'
	option netmask '255.255.255.0'

config interface 'GE5'
	option ifname 'eth11'
	option proto 'dhcp'
	option hostname 'vc-ge5'

config interface 'GE6'
	option ifname 'eth10'
	option proto 'dhcp'
	option hostname 'vc-ge6'

config interface 'GE7'
	option ifname 'eth9'
	option proto 'dhcp'
	option hostname 'vc-ge7'

config interface 'GE8'
	option ifname 'eth5'
	option proto 'dhcp'
	option hostname 'vc-ge8'

config interface 'SFP1'
	option ifname 'eth3'
	option proto 'dhcp'
	option hostname 'vc-sfp1'

config interface 'SFP2'
	option ifname 'eth2'
	option proto 'dhcp'
	option hostname 'vc-sfp2'
