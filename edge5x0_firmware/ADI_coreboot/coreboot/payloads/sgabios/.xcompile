# TARCH_SEARCH=/cygdrive/c/sage_edk/tools/xgcc/bin/i386-elf- i386-elf- /cygdrive/c/sage_edk/tools/xgcc/bin/x86_64-elf- x86_64-elf-
# elf32-i386 toolchain
GCCPREFIX=/cygdrive/c/sage_edk/tools/xgcc/bin/i386-elf-

export AS:=/cygdrive/c/sage_edk/tools/xgcc/bin/i386-elf-as 
export CC:=/cygdrive/c/sage_edk/tools/xgcc/bin/i386-elf-gcc -Wa,--divide -fno-stack-protector -Wl,--build-id=none -Wno-unused-but-set-variable 
export CPP:=/cygdrive/c/sage_edk/tools/xgcc/bin/i386-elf-cpp
export AR:=/cygdrive/c/sage_edk/tools/xgcc/bin/i386-elf-ar
export LD:=/cygdrive/c/sage_edk/tools/xgcc/bin/i386-elf-ld 
export STRIP:=/cygdrive/c/sage_edk/tools/xgcc/bin/i386-elf-strip
export NM:=/cygdrive/c/sage_edk/tools/xgcc/bin/i386-elf-nm
export OBJCOPY:=/cygdrive/c/sage_edk/tools/xgcc/bin/i386-elf-objcopy
export OBJDUMP:=/cygdrive/c/sage_edk/tools/xgcc/bin/i386-elf-objdump

export IASL:=/cygdrive/c/sage_edk/tools/xgcc/bin/iasl

# native toolchain
export HOSTCC:=gcc
